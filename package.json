{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.3.7",
    "@fontsource/inter": "4.1.0",
    "@glenstack/cf-workers-graphql": "1.0.4",
    "@graphql-tools/schema": "6.0.18",
    "@headlessui/react": "0.2.0",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.3",
    "@testing-library/user-event": "12.6.0",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.22",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "classnames": "2.2.6",
    "graphql-relay": "0.6.0",
    "graphql-relay-tools": "0.1.1",
    "graphql-scalars": "1.7.0",
    "graphql-tag": "2.11.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.1",
    "typescript": "4.1.3",
    "web-vitals": "1.1.0"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "build:fab": "npm run build && npm run fab:build",
    "lint": "eslint --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "check-types": "tsc",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|ts|tsx|json|json5|md|css)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "concurrently \"npm:check-types\" \"npm:check-format\" \"npm:lint\" \"npm:build\"",
    "fab:build": "fab build",
    "fab:serve": "fab serve fab.zip",
    "pretest": "npm run validate",
    "test": "craco test",
    "eject": "react-scripts eject",
    "graphql:update-schema": "apollo client:download-schema --endpoint='http://localhost:3000/graphql' ./schema.graphql",
    "graphql:update-types": "apollo client:codegen --target=typescript --localSchemaFile='./schema.graphql' --includes='./src/**/*'",
    "graphql:watch-types": "npm run graphql:update-types -- --watch"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "6.0.0",
    "@fab/actions": "1.0.0-rc.9",
    "@fab/cli": "1.0.0-rc.9",
    "@fab/deployer-cf-workers": "^1.0.0-rc.9",
    "@fab/input-static": "1.0.0-rc.9",
    "@fab/plugin-precompile": "1.0.0-rc.9.p1",
    "@fab/plugin-render-html": "1.0.0-rc.9",
    "@fab/plugin-rewire-assets": "1.0.0-rc.9",
    "@fab/server": "1.0.0-rc.9",
    "@tailwindcss/forms": "0.2.1",
    "@tailwindcss/line-clamp": "0.1.0",
    "@tailwindcss/postcss7-compat": "2.0.2",
    "@types/classnames": "2.2.11",
    "@types/react-router-dom": "5.1.7",
    "apollo": "2.32.1",
    "autoprefixer": "9.8.6",
    "concurrently": "5.3.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.3",
    "postcss": "7.0.35",
    "prettier": "1.19.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@2.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check-types && lint-staged"
    }
  },
  "lint-staged": {
    ".+(js|jsx|ts|tsx)": [
      "eslint"
    ],
    "**/*.+(js|jsx|ts|tsx|json|json5|md|css)": [
      "prettier --write"
    ]
  }
}
